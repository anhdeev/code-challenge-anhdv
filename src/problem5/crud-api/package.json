{
  "name": "crud-api",
  "version": "0.1.0",
  "description": "REST API with Node JS, TypeScript, Express and Prisma",
  "main": "src/index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "predev": "kill -9 $(lsof -ti:3001) || exit 0",
    "test": "jest --coverage",
    "test:unit": "jest --coverage tests/unittest",
    "test:integration": "jest --coverage tests/integration",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "db:new": "node scripts/migrateNew.js",
    "db:init": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "prepare": "husky install",
    "build": "npx prisma generate && rimraf build && tsc -p tsconfig.json",
    "start": "node build/src/index.js",
    "swagger": "node src/swaggerAutogen.js"
  },
  "keywords": [
    "node",
    "node.js",
    "typescript",
    "boilerplate",
    "express",
    "rest",
    "api",
    "prisma",
    "postgresql",
    "es6",
    "es7",
    "es8",
    "es9",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "license": "UNLICENSED",
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@jest/globals": "^29.3.1",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.13",
    "@types/express-session": "^1.17.9",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^22.10.6",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.37",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/xss-filters": "^0.0.27",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^13.1.0",
    "node-mocks-http": "^1.12.1",
    "nodemon": "^3.1.9",
    "prettier": "^2.8.1",
    "prisma": "^5.5.2",
    "supertest": "^6.3.3",
    "swagger-autogen": "^2.23.7",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^5.0.0",
    "@types/passport": "^1.0.17",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.1",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "google-auth-library": "^9.2.0",
    "helmet": "^6.0.1",
    "http-status": "^2.1.0",
    "joi": "^17.7.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "query-string": "^8.1.0",
    "sqlite3": "^5.1.7",
    "winston": "^3.8.2"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seeds.ts"
  }
}
